{% trans_default_domain('field.tire.cartype') %}

{# Легковой  #}
{% if value == 'passenger' %}

    <div data-bg="data:image/svg+xml;base64,"
         class="lazy bg-contain"
         data-bs-toggle="tooltip"
         data-bs-placement="bottom"
         data-bs-delay='{"show":600,"hide":100}'
         style="width: 30px; height: 30px;"
         title="{{ 'label'|trans }} {{ 'passenger'|trans }}"
    >
    </div>


{#  Джип #}
{% elseif value == 'jeep' %}

    <div data-bg="data:image/svg+xml;base64,"
         class="lazy bg-contain"
         data-bs-toggle="tooltip"
         data-bs-placement="bottom"
         data-bs-delay='{"show":600,"hide":100}'
         style="width: 28px; height: 28px;"
         title="{{ 'label'|trans }} {{ 'jeep'|trans }}"
    >
    </div>


{#  Автобус #}
{% elseif value == 'bus' %}

    <div data-bg="data:image/svg+xml;base64,"
         class="lazy bg-contain"
         data-bs-toggle="tooltip"
         data-bs-placement="bottom"
         data-bs-delay='{"show":600,"hide":100}'
         style="width: 22px; height: 22px;"
         title="{{ 'label'|trans }} {{ 'bus'|trans }}"
    >
    </div>


{#  Грузовик #}
{% elseif value == 'truck' %}

    <div data-bg="data:image/svg+xml;base64,"
         class="lazy bg-contain"
         data-bs-toggle="tooltip"
         data-bs-placement="bottom"
         data-bs-delay='{"show":600,"hide":100}'
         style="width: 16px; height: 20px;"
         title="{{ 'label'|trans }} {{ 'truck'|trans }}"
    >
    </div>

{% endif %}