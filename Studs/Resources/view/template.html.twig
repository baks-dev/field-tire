{% trans_default_domain 'field.tire.studs' %}
<div class='d-flex gap-3 align-items-center'>
	
	{% if value %}
		<img data-src="data:image/svg+xml;base64,"
				width="18" height="18"
				src="/assets/img/empty.png"
				class="lazy"
				alt="{{ 'label'|trans }}"
				title="{{ 'label'|trans }}"
		/>
		<span>{{ 'label'|trans }} </span>
	{% endif %}

</div>